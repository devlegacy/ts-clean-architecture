{
  "name": "ts-clean-architecture",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run build:clean",
    "prepare": "husky install",
    "build": "npm run tsc:build",
    "postbuild": "cpy src/**/*.json dist",
    "build:clean": "rm -f tsconfig.build.tsbuildinfo && rm -Rf ./dist/* ./.tmp/compiled/ ; exit 0",
    "tsc:build": "tsc -p tsconfig.build.json && resolve-tspaths -p tsconfig.json -s ./src -o ./dist",
    "start:bank:window:console": "tsnd -r ts-node/register/transpile-only -r tsconfig-paths/register ./src/apps/bank/console/window/main.ts",
    "start:bank:atm:console": "tsnd -r ts-node/register/transpile-only -r tsconfig-paths/register ./src/apps/bank/console/atm/main.ts",
    "start:mooc:api": "node ./dist/apps/mooc/backend/main.js",
    "start:user:console:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/apps/user/console/main.ts",
    "start:user:api:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/apps/user/backend/main.ts",
    "start:user:graph:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/apps/user/graphql/main.ts",
    "start:mooc:api:debug": "tsnd --cache-directory=./.tmp --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/apps/mooc/backend/main.ts",
    "start:backoffice:api:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/apps/backoffice/backend/main.ts",
    "start:land:api:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/apps/land/backend/main.ts",
    "command:mooc:rabbitmq": "cross-env APP_ENV=production ts-node -r tsconfig-paths/register -r ts-node/register/transpile-only ./src/apps/mooc/backend/command/runConfigureRabbitMQCommand",
    "command:backoffice:rabbitmq": "cross-env APP_ENV=production ts-node -r tsconfig-paths/register -r ts-node/register/transpile-only ./src/apps/backoffice/backend/command/runConfigureRabbitMQCommand",
    "prune:mooc": "ts-prune --project tsconfig.build.json && ts-unused-exports tsconfig.build.json",
    "prune:mooc:filter": "ts-prune --project tsconfig.build.json | grep -v '(used in module)'",
    "lint": "eslint --color ./src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "nibble": "eslint-nibble ./src/",
    "test": "npm run test:unit && npm run test:features",
    "test:unit": "cross-env APP_ENV=test jest --runInBand --detectOpenHandles",
    "test:features": "npm run test:mooc:backend:features && npm run test:backoffice:backend:features",
    "test:mooc:backend:features": "cross-env APP_ENV=test cucumber-js -p mooc_backend",
    "test:backoffice:backend:features": "cross-env APP_ENV=test cucumber-js -p backoffice_backend",
    "test:mooc:backend:load": "autocannon http://127.0.0.1:8080/status/",
    "test:mooc:backend:load:https": "autocannon https://127.0.0.1:8080/status/",
    "posttest": "find ./src/ -name '*.repo' -delete && rm -Rf ./dist/*",
    "package:clean": "rm -Rf ./node_modules && rm -f ./package-lock.json && rm -f ./yarn.lock",
    "package:check": "depcheck",
    "package:install": "npm install && npm audit fix",
    "package:install:legacy": "npm install --legacy-peer-deps && npm audit fix --legacy-peer-deps",
    "package:refresh": "npm run package:clean && npm run package:update",
    "package:refresh:legacy": "npm run package:clean && npm run package:update:legacy",
    "package:update": "ncu -u && npm run package:install",
    "package:update:minor": "ncu -u --target=minor  && npm run package:install",
    "package:update:minor:legacy": "ncu -u --target=minor && npm run package:install:legacy",
    "package:update:legacy": "ncu -u && npm run package:install:legacy",
    "package:update:interactive": "ncu --interactive"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
