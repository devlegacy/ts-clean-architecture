{
  "name": "ts-clean-architecture",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "npm run tsc:build",
    "build:clean": "rm -f tsconfig.build.tsbuildinfo && rm -Rf ./dist/* ; exit 0",
    "start": "node ./dist/infrastructure/driving-adapters/api-rest/main.js",
    "start:console": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register ./src/infrastructure/driving-adapters/console/console.ts",
    "start:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/infrastructure/driving-adapters/api-rest/main.ts",
    "tsc:build": "tsc -p tsconfig.build.json && resolve-tspaths -p tsconfig.json -s ./src -o ./dist",
    "test": "jest --runInBand --detectOpenHandles",
    "prepare": "husky install",
    "lint": "eslint --color ./src/**/*.ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "Samuel R. <samuel@devexteam.com> (https://devexteam.com)",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^6.0.0",
    "@fastify/cookie": "^7.0.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^9.1.0",
    "@fastify/rate-limit": "^7.0.0",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "fastify": "^4.0.0",
    "mongodb": "^4.7.0",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "resolve-tspaths": "^0.6.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.3"
  },
  "optionalDependencies": {
    "npm-check-updates": "^13.1.3"
  }
}
