{
  "name": "ts-clean-architecture",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "npm run tsc:build",
    "build:clean": "rm -f tsconfig.build.tsbuildinfo && rm -Rf ./dist/* ; exit 0",
    "start:console": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register ./src/infrastructure/driving-adapters/console/console.ts",
    "start:debug": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register --inspect -- ./src/infrastructure/driving-adapters/api-rest/main.ts",
    "tsc:build": "tsc -p tsconfig.build.json && resolve-tspaths -p tsconfig.json -s ./src -o ./dist",
    "test": "jest --runInBand --detectOpenHandles",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Samuel R. <samuel@devexteam.com> (https://devexteam.com)",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "fastify": "^3.29.0",
    "mongodb": "^4.5.0",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "resolve-tspaths": "^0.3.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  },
  "optionalDependencies": {
    "npm-check-updates": "^12.5.11"
  }
}
