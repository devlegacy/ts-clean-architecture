  "version": "0.2.0",
  "configurations": [
    {
      "name": "üîÑ Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**/*.js",
        "<node_internals>/**/*.js",
        "<node_internals>/internal/async_hooks.js",
        "<node_internals>/internal/inspector_async_hook.js"
      ],
      // https://github.com/pinojs/pino/issues/441
      // "outputCapture": "std",
      "type": "node",
      "restart": true
    },
    {
      // NOTE: avoid use with dist folder
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "name": "üöÄ TS: Launch Program",
      "program": "${file}",
      "request": "launch",
      // "runtimeArgs": ["--nolazy", "-r", "ts-node/register", "-r", "tsconfig-paths/register"],
      "runtimeArgs": [
        "--nolazy",
        "--trace-deprecation",
        // "--abort-on-uncaught-exception",
        "-r",
        "${workspaceFolder}/node_modules/ts-node/register/transpile-only",
        "-r",
        "${workspaceFolder}/node_modules/tsconfig-paths/register"
      ],
      // "args": ["${workspaceFolder}/src/index.ts"],
      "runtimeExecutable": "node",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**",
        "${workspaceFolder}/dist/**",
        "${workspaceFolder}/.tmp/**"
      ],
      "type": "node",
      // https://github.com/pinojs/pino/issues/441
      "outputCapture": "std",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json",
        "APP_ENV": "test",
        "TZ": "utc"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "üêû Jest: Debug current file",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "args": [
        "${fileBasenameNoExtension}",
        "--config",
        "jest.config.ts",
        // "--inspect-brk",
        // "-r",
        // "ts-node/register/transpile-only",
        // "-r",
        // "tsconfig-paths/register",
        // "${workspaceRoot}/node_modules/.bin/jest",
        // "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand",
        "--detectOpenHandles"
        // "--config",
        // "${workspaceRoot}/jest.config.js",
        // "-t",
        // "${file}"
      ],
      "windows": {
        // "type": "node",
        // "request": "launch",
        // "name": "üêû Jest: Debug current file",
        "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "args": [
          "--runInBand",
          "--detectOpenHandles",
          "//${relativeFile}" // the // is a hack for windows files
        ],
        "env": {
          "APP_ENV": "test"
        }
      },
      // https://github.com/pinojs/pino/issues/441
      "outputCapture": "std",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**/*.js"
        // "<node_internals>/**/*.js",
        // "<node_internals>/internal/async_hooks.js",
        // "<node_internals>/internal/inspector_async_hook.js"
      ],
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json"
      }
    },
    {
      "name": "üë• Run User console app",
      "request": "launch",
      // "envFile": "${workspaceFolder}/.env",
      // "preLaunchTask": null,
      "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
      "restart": true,
      "runtimeArgs": ["run-script", "start:user:console:debug", "--"],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**/*.js"
        // "<node_internals>/**/*.js",
        // "<node_internals>/internal/async_hooks.js",
        // "<node_internals>/internal/inspector_async_hook.js"
      ],
      "type": "node",
      // https://github.com/pinojs/pino/issues/441
      "outputCapture": "std",
      // "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
