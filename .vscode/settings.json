{
  "files.exclude": {
    ".cache": true,
    ".commitlintrc.js": true,
    ".czrc": true,
    ".data": true,
    ".dockerignore": true,
    ".editorconfig": true,
    ".eslintignore": true,
    ".eslintrc.js": true,
    ".eslintrc": true,
    ".husky": true,
    ".idea": true,
    ".lintstagedrc.json": true,
    ".npmrc": true,
    ".nvmrc": true,
    ".php_cs": true,
    ".prettierignore": true,
    ".prettierrc": true,
    ".tmp": true,
    ".vscode": false,
    "*.example": true,
    "*.xml": true,
    // "*.yml": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.code-search": true,
    "**/*.map": true,
    "**/bower_components/": true,
    "**/CVS": true,
    "**/node_modules/": true,
    "*git*": true,
    "*lock*": true, // lock files
    "ABOUT.md": true,
    "CHANGELOG.md": true,
    "coverage": true,
    "cucumber.js": true,
    "dist": true,
    "docs": true,
    "jest.config.[j|t]s": true,
    "license": true,
    "LICENSE": true,
    "nbproject": true,
    "nodemon.json": true,
    // "tests": true,
    "tsconfig*.json": true,
    "tsconfig*.tsbuildinfo": true,
    "types": true,
    "yarn-error.log": true
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "files.eol": "\n",
  "prettier.requireConfig": true
}
