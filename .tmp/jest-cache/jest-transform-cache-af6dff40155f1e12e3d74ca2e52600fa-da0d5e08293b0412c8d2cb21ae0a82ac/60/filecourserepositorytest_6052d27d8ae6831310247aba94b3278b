const course_1 = require("@/contexts/mooc/courses/domain/course");
const file_course_repository_1 = require("@/contexts/mooc/courses/infrastructure/persistance/file-course.repository");
describe('File Course Repository', () => {
    it('should save a course', async () => {
        const expectedCourse = new course_1.Course('id', 'name', 'duration');
        const repository = new file_course_repository_1.FileCourseRepository();
